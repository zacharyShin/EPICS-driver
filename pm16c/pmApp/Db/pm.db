record(stringin, "$(user):Ver")
{
	field(DESC, "firmware version")
	field(DTYP, "stream")
	field(INP,  "@pm.proto VER pm")
	field(SCAN, "1 second")
}
record(bi, "$(user):R_Loc-Rem")
{
	field(DESC, "read Loc-Rem status")
        field(DTYP, "stream")
	field(INP,  "@pm.proto LRS pm")
        field(SCAN, "1 second")
	field(ZNAM, "L")
	field(ONAM, "R")
}

record(bo, "$(user):S_Loc-Rem")
{
        field(DESC, "set Loc-Rem mode")
        field(DTYP, "stream")
        field(OUT,  "@pm.proto SLR pm")
	field(OMSL, "supervisory")
	field(ZNAM, "LOC")
	field(ONAM, "REM")
}

