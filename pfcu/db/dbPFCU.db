record(calcout, "$(user):PFCUmask")
{
	field(DESC, "PFCU filter REMOVE mask")
#	field(DTYP, "stream")
	field(CALC, "1000*A+100*B+10*C+D")
	field(SCAN, "Passive")
	field(INPA, "$(user):filterMASK1.VAL PP NMS")
	field(INPB, "$(user):filterMASK2.VAL PP NMS")
	field(INPC, "$(user):filterMASK3.VAL PP NMS")
	field(INPD, "$(user):filterMASK4.VAL PP NMS")
}

record(bo, "$(user):callPFCU")
{
	field(DESC,  "call PFCU to insert and remove filters")
	field(DTYP,  "stream")
	field(OUT,   "@pfcu.proto callPFCU($(user)) pfcu")
}
