record(bo, "$(user):PFCUbo$(no)")
{
	field(DESC,  "PFCU filter BO$(no)")
	field(DTYP,  "Soft Channel")
	field(ONAM,  "Remove")
	field(ZNAM,  "Insert")
	field(FLNK,  "$(user):filterMASK$(no)")
}

record(calc, "$(user):filterMASK$(no)")
{
	field(DESC,  "PFCU filter$(no) mask")
	field(SCAN,  "Passive")
	field(CALC,  "(1-A)*$(no)")
	field(INPA,  "$(user):PFCUbo$(no).VAL PP MS")
	field(FLNK,  "$(user):PFCUmask")
}

record(bo, "$(user):debugTest")
{
	field(DESC,  "test PV")
	field(DTYP,  "stream")
	field(OUT,   "@pfcu.proto debugTest($(user)) pfcu")
	field(SCAN,  "Passive")
	field(ONAM,  "High")
	field(ZNAM,  "Low")
}

